    decorator = ""
    args = []
    if self.info.is_static_method():
        decorator = "@staticmethod\n"
    if (
        self.info.is_method()
        or self.info.is_constructor()
        or self.info.is_instance()
    ):
        args.append("self")
    args.extend(self.info.get_passed_args())
    definition = "{}def {}({}):\n    pass\n".format(
        decorator,
        self.name,
        ", ".join(args),
    )
    return definition

