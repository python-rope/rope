    if amount == 0:
        return source_code
    lines = source_code.splitlines(True)
    result = []
    for l in lines:
        if l.strip() == "":
            result.append("\n")
            continue
        if amount &lt; 0:
            indents = codeanalyze.count_line_indents(l)
            result.append(max(0, indents + amount) * " " + l.lstrip())
