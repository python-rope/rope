    function = self.function_name
    if self.implicit_arg:
        function = self.args[0] + "." + self.function_name
    params = []
    start = 0
    if self.implicit_arg or self.constructor:
        start = 1
    if self.args[start:]:
        params.extend(self.args[start:])
    if self.keywords:
