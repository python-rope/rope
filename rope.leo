<t tx="ekr.20221118072831.655">def _count_needed_parens(self, children):
    start = 0
    opens = 0
    for child in children:
        if not isinstance(child, basestring):
            continue
        if child == "" or child[0] in "'\"":
            continue
        index = 0
        while index &lt; len(child):
            if child[index] == ")":
                if opens &gt; 0:
                    opens -= 1
                else:
